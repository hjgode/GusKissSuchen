{
  "manifest_version": 2,
  "name": "Kundennr. in Guskiss suchen",
  "version": "1.33",
 
  "description": "Presents a right-click menu option to search a text at Guskiss in a new tab.",
  "icons": {
    "48": "icons/logo.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "contextMenuGussKiss10@example.com",
       "strict_min_version": "42.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },
 
  "permissions": ["contextMenus"]
}

